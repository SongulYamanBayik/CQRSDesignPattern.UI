@model List<CQRSDesignPattern.UI.CQRS.Results.GetProductProducerQueryResult>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />


<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Ürün Adı</th>
            <th>Depo</th>
            <th>Durum</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductID</td>
                <td>@item.ProductName</td>
                <td>@item.Storage</td>
                <td>@item.Status</td>
            </tr>
        }
    </table>
</div>